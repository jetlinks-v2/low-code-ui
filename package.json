{
  "name": "jetlinks-ui-cli",
  "private": true,
  "scripts": {
    "build": "pnpm --filter scripts build",
    "dev": "pnpm --filter scripts dev",
    "turbo:build": "turbo run build --parallel",
    "turbo:dev": "turbo run dev --parallel",
    "stub": "pnpm --filter @jetlinks/** prepack",
    "clean": "pnpm turbo run clean && rimraf node_modules",
    "changeset": "changeset",
    "version": "pnpm run changeset version",
    "release:only": "changeset publish --registry=https://registry.jetlinks.cn/"
  },
  "devDependencies": {
    "@jetlinks/lint": "workspace:^",
    "@jetlinks/tsconfig": "workspace:*",
    "@jetlinks/types": "workspace:^",
    "@turbo/gen": "^1.10.12",
    "@types/fs-extra": "^11.0.1",
    "@types/lodash-es": "^4.17.8",
    "@types/node": "^20.4.5",
    "eslint": "^7.32.0",
    "esno": "^0.16.3",
    "fs-extra": "^11.1.1",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "turbo": "^1.10.12",
    "typescript": "^5.1.6",
    "unbuild": "^1.2.1"
  },
  "packageManager": "pnpm@7.15.0",
  "dependencies": {
    "@changesets/cli": "^2.26.2",
    "@jetlinks/utils": "workspace:^",
    "jetlinks-ui-components": "^1.0.31",
    "latest": "^0.2.0",
    "lodash-es": "^4.17.21",
    "pinia-plugin-persistedstate": "^3.2.0",
    "pubsub-js": "^1.9.4"
  }
}
